{
    "name": "lmsystems",
    "version": "0.0.4",
    "description": "SDK for integrating purchased graphs from the lmsystems marketplace",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "engines": {
        "node": ">=18"
    },
    "scripts": {
        "prepare": "npm run build",
        "prepublish": "npm run clean && npm run build",
        "build": "tsc --module ES2020",
        "test": "jest",
        "start:example": "tsx examples/usage.ts",
        "clean": "rm -rf dist/",
        "lint": "eslint --cache --ext .ts src/",
        "format": "prettier --write \"src/**/*.ts\"",
        "prepublish": "npm run clean && npm run build"
    },
    "dependencies": {
        "@langchain/core": ">=0.2.36 <0.3.0 || >=0.3.9 < 0.4.0",
        "@langchain/langgraph": "^0.2.34",
        "@langchain/langgraph-sdk": "^0.0.32",
        "axios": "^1.6.0",
        "jsonwebtoken": "^9.0.0"
    },
    "devDependencies": {
        "@types/jsonwebtoken": "^9.0.0",
        "@types/node": "^22.10.2",
        "@typescript-eslint/eslint-plugin": "^6.12.0",
        "@typescript-eslint/parser": "^6.12.0",
        "dotenv": "^16.0.0",
        "eslint": "^8.33.0",
        "eslint-config-prettier": "^8.6.0",
        "jest": "^29.5.0",
        "prettier": "^2.8.3",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.0",
        "tsx": "^4.7.0",
        "typescript": "^5.0.0"
    },
    "peerDependencies": {
        "@langchain/core": ">=0.2.36 <0.3.0 || >=0.3.9 < 0.4.0"
    },
    "files": [
        "dist",
        "README.md"
    ],
    "author": "Sean Sullivan",
    "license": "MIT"
}
